{
  "name": "resource-endpoint",
  "version": "1.0.0-beta.2",
  "license": "MIT",
  "description": "A simple wrapper for any Promise based HTTP client.",
  "author": "Eric Dowell <github@ericdowell.com> (https://ericdowell.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericdowell/resource-endpoint.git"
  },
  "bugs": {
    "url": "https://github.com/ericdowell/resource-endpoint/issues"
  },
  "homepage": "https://github.com/ericdowell/resource-endpoint#readme",
  "keywords": [
    "json",
    "api",
    "promise",
    "client",
    "wrapper",
    "http",
    "simple",
    "endpoint",
    "resource",
    "crud",
    "laravel",
    "axios"
  ],
  "main": "lib/resource-endpoint.js",
  "types": "lib/",
  "scripts": {
    "prebuild": "npm run lint -- --quiet && rm -fr ./lib",
    "build": "npm run prebuild && npm run build:ts && npm run build:webpack && npm run postbuild",
    "build:ts": "cross-env NODE_ENV=production tsc",
    "build:webpack": "cross-env NODE_ENV=production webpack",
    "postbuild": "rm -fr ./lib/__tests__",
    "lint": "tsc --noEmit && eslint ./src --ext .ts",
    "lint:fix": "npm run lint -- --fix --quiet",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "npm run lint:fix"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "@types/qs": "^6.9.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "axios": "^0.19.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "qs": "^6.9.1",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
